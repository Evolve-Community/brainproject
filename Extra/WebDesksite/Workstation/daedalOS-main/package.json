{
  "name": "daedalos",
  "alias": "daedalOS",
  "version": "2.0.0",
  "description": "Desktop environment in the browser",
  "author": "Dustin Brett",
  "license": "MIT",
  "browserslist": [
    "defaults and supports es6 and not ie 11"
  ],
  "scripts": {
    "build": "npm run build:fs && next build",
    "build:fs": "cd public && node ../scripts/fs2json.js --exclude .index --out .index/fs.9p.json . && node ../node_modules/browserfs/dist/scripts/make_http_index.js > .index/fs.bfs.json",
    "build:robots": "node scripts/robots.js",
    "docker:build": "docker build -t daedalos .",
    "docker:run": "docker run -dp 3000:3000 --rm --name daedalos daedalos",
    "dev": "next dev",
    "eslint": "eslint --report-unused-disable-directives .",
    "export": "next export",
    "prepare": "husky install",
    "prettier": "prettier --write .",
    "start": "next start",
    "stylelint": "stylelint --formatter=verbose **/*.ts*",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write",
    "*.{ts,tsx}": "stylelint --fix",
    "*.{js,ts,tsx}": "eslint --fix"
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.3.1",
    "@panzoom/panzoom": "^4.4.4",
    "@wasmer/wasi": "0.12.0",
    "@wasmer/wasm-terminal": "^0.12.0",
    "@wasmer/wasm-transformer": "^0.12.0",
    "@wasmer/wasmfs": "^0.12.0",
    "clsx": "^1.1.1",
    "fflate": "^0.7.3",
    "framer-motion": "^6.2.3",
    "html-to-image": "^1.9.0",
    "idb-keyval": "^6.1.0",
    "ini": "^2.0.0",
    "isomorphic-git": "^1.11.0",
    "music-metadata-browser": "^2.5.3",
    "next": "^12.0.8",
    "node-unrar-js": "^1.0.3",
    "polished": "^4.1.4",
    "prettier": "^2.5.1",
    "react": "^18.0.0-rc.0",
    "react-dom": "^18.0.0-rc.0",
    "react-rnd": "^10.3.5",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^12.0.8",
    "@stylelint/postcss-css-in-js": "^0.37.2",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^13.0.0-alpha.5",
    "@types/ini": "^1.3.31",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.12",
    "@types/offscreencanvas": "^2019.6.4",
    "@types/react": "^17.0.38",
    "@types/styled-components": "^5.1.21",
    "@types/video.js": "^7.3.30",
    "@types/wicg-file-system-access": "^2020.9.4",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "babel-plugin-styled-components": "^2.0.2",
    "browserfs": "https://github.com/jvilk/BrowserFS.git",
    "emulators": "^0.72.1",
    "emulators-ui": "^0.72.9",
    "eruda": "https://github.com/liriliri/eruda",
    "eslint": "^8.7.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^12.0.8",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.11.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-testing-library": "^5.0.4",
    "eslint-plugin-unicorn": "^40.1.0",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "jest-next-dynamic": "^1.0.1",
    "jsdom-worker": "^0.2.1",
    "lint-staged": "^12.3.1",
    "monaco-editor": "^0.31.1",
    "pdfjs-dist": "^2.12.313",
    "postcss-syntax": "^0.36.2",
    "stylelint": "^14.3.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "tinymce": "^5.10.2",
    "typescript": "^4.5.5",
    "webamp": "^1.5.0",
    "xterm": "^4.16.0"
  }
}
